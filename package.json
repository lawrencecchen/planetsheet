{
  "name": "planetsheet",
  "version": "0.1.9",
  "description": "The most user friendly database tool.",
  "keywords": [
    "postgres",
    "mysql"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:unocss": "npm run generate:unocss -- --watch",
    "build": "run-s build:*",
    "build:unocss": "npm run generate:unocss",
    "build:next": "next build",
    "build:prepare_psheet": "tsx ./utils/prepare_psheet.ts",
    "start": "next start -p 54337",
    "lint": "next lint",
    "generate:unocss": "unocss \"./src/**/*.{ts,tsx,jsx,js}\" --out-file ./styles/uno.css",
    "typecheck": "tsc -b",
    "prettier": "prettier --write app",
    "format": "prettier --write .",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "cd psheet && npm publish"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@glideapps/glide-data-grid": "^4.2.1",
    "@iconify/json": "^2.1.77",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-accordion": "^0.1.6",
    "@tailwindcss/forms": "^0.5.2",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-table": "8.2.6",
    "@tanstack/react-virtual": "3.0.0-beta.2",
    "@trpc/client": "^9.25.3",
    "@trpc/next": "^9.25.3",
    "@trpc/react": "^9.25.3",
    "@trpc/server": "^9.25.3",
    "better-sqlite3": "^7.6.1",
    "clsx": "^1.2.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.5.0",
    "fuse.js": "^6.6.2",
    "immer": "^9.0.15",
    "knex": "^2.1.0",
    "konva": "^8.3.10",
    "next": "12.2.2",
    "pg": "^8.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-konva": "^18.2.1",
    "react-query": "^3.39.1",
    "superjson": "^1.9.1",
    "tailwindcss-radix": "^2.3.1",
    "tiny-invariant": "^1.2.0",
    "usehooks-ts": "^2.6.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.0.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.25",
    "@unocss/cli": "^0.44.1",
    "@unocss/preset-icons": "^0.44.1",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.18",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "react-is": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.6",
